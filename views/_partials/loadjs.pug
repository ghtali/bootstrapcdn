-var bootstrap = config.bootstrap4.filter(function(o) { return o.current; })[0];
-var jquery = config.javascript.filter(function(o) { return o.name === 'jquery'; })[0];

if (helpers.theme.selected(config, theme) === config.theme)
    | loadjs(['#{bootswatch.uri}', 'css!https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,700'], 'styles');

| loadjs(['#{jquery.uri}', '#{bootstrap.javascriptBundle}'], 'jquery', {
|     async: false,
|     numRetries: 3
| });
| loadjs.ready('jquery', {
|     success: function() {
|         $('.ads-info-toggler').popover();
|     }
| });
